#!/usr/bin/env dash

# skip version
read -r line || exit 1
printf "%s\n" "$line"

# skip opening bracket
read -r line || exit 1
printf "%s\n" "$line"

read -r line || exit 1
printf "%s\n" "$(printf "%s\n" "$line" | i3.status.customize_line)"

while :
do
	read -r line || exit 1
	printf ",%s\n" "$(printf "%s" "$line" | tail -c+2 | i3.status.customize_line.extra)"
done
